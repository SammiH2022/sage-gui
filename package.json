  },
  "license": "UNLICENSED",
  "devDependencies": {
    "process": "^0.11.10",
    "standard-version": "^9.3.0"
  },
  "standard-version": {
    "header": "# Changelog\n\nThe following are some notable changes to the UI\n\n"
  },
  "workspaces": [
    "apps/admin",
    "apps/sage",
    "apps/project",
    "apps/registration-api"
  ],
  "dependencies": {
    "concurrently": "^7.3.0"
  }
}
